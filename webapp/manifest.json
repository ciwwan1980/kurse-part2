{
	"version": "1.25.0",
	"type": "application",
	"_version": "1.9.0",
	"sap.app": {
		"_version": "1.3.0",
		"id": "ajil.app",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{app Title}}",
		"description": "{{appDescription}}",
		"ach": "Fin.AR.ACC"
	},
	"sap.ui": {
		"_version": "1.3.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_belize", "sap-bluecrystal"]
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.70.0",
			"libs": {
				"sap.m": {
					"minVersion": "1.70.0"
				},
				"sap.ui.layout": {
					"minVersion": "1.71.0"
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},

		"rootView":{
			"viewName":"ajil.app.view.App", 
			"type":"XML"
		},
		"routing":{
			"config":{
				"routerClass":"sap.m.routing.Router", 
				"viewPath":"ajil.app.view",
				"viewType":"XML", 
				"controlId":"idSpidy"
			},
			"routes":[{
				"pattern": "",
				"name": "home",
				"target":["modi","empty"]
			}, 
			{
				"pattern": "joe/{kaven}",
				"name": "detail",
				"target":"joe"
			}], 
			"targets":{
				"modi":{
					"viewName":"View1", 
					"viewId":"idModi",
					"controlAggregation":"masterPages"
				},
				"joe":{
					"viewName":"View2", 
					"viewId":"idBiden",
					"controlAggregation":"detailPages"
				}, 
				"empty":{
					"viewName":"Empty", 
					"viewId":"idEmpty",
					"controlAggregation":"detailPages"
				}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {},
				"preload": false,
				"async": true,
				"uri": "i18n/i18n.properties"
			},
			"local": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"uri": "models/mockdata/fruits.json",
				"preload": true
			}
		}
	}
}